#include    <stdio.h>
#include    <stdlib.h>
#include    <string.h>

int main(){
    char line[40];
    int cnt=0;

    while(fgets(line,40,stdin)){
        cnt=0;
        for(int i=0;line[i]!='\0';i++){
            if(line[i]=='2'){
                cnt++;
            }
        }
        printf("%d\n",cnt);
    }
}

