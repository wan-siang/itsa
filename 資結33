#include    <stdio.h>
#include    <stdlib.h>

struct tnode{
    int num;
    struct tnode *LChild;
    struct tnode *RChild;
};

struct tnode *insert(struct tnode *tree, int num);
int height(struct tnode *T);

int main(){
    struct tnode *tree = NULL;
    int num;

    while(scanf("%d", &num)!=EOF){
        tree = insert(tree,num);
    }

    int h = height(tree);
    printf("%d\n",h);
}

int height(struct tnode *T){
    if(T==NULL){
        return 0;
    }else{
        int Lheight = height(T->LChild);
        int Rheight = height(T->RChild);
 
        if(Lheight > Rheight) return (Lheight+1);
        else return(Rheight+1);
    }
}

struct tnode *insert(struct tnode *T, int num){
    if(T==NULL){
        struct tnode *p = (struct tnode *)malloc(sizeof(struct tnode *));
        p->num = num;
        p->LChild = NULL;
        p->RChild = NULL;
        return p;
    }
    if(num < T->num){
        T->LChild = insert(T->LChild,num);
    } else {
        T->RChild = insert(T->RChild,num);
    }
    return T;

}
