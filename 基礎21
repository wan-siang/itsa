#include    <stdio.h>

int main(){
    float number;
    float min;
    float max;

    scanf("%f",&number);
    min = number;
    max = number;

    for(int i=0;i<9;i++){
        scanf("%f",&number);
        if(number>max){
            max = number;
        }else if(number<min){
            min = number;
        }
    }
    printf("maximum:%.2f\n",max);
    printf("minimum:%.2f\n",min);
}
